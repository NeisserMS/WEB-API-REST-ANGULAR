
<script src="~/Scripts/angular.js"></script>
<script src="~/Scripts/App.js"></script>
<!DOCTYPE html>
<html ng-app="personaModule">
<head>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/App.js"></script>
</head>
<body>
    <div ng-controller="personaController">
        <table class="table">
            <tr>
                <th>
                    nombre
                </th>
                <th>
                    apellido
                </th>
                <th>
                    edad
                </th>
                <th></th>
            </tr>

         
                <tr ng-repeat="p in personitas">
                    <td>
                       {{p.nombre}}
                    </td>
                    <td>
                        {{p.apellido}}
                    </td>
                    <td>
                        {{p.edad}}
                    </td>
                    
                </tr>
            

        </table>
    </div>
</body>
</html>